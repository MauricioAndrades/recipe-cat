{
  "env": {
    "NODE_ENV": "development"
  },
  "events": {
    "restart": "osascript -e 'display notification \"App restarted due to:'$FILENAME'\" with title \"nodemon\"'"
  },
  "execMap": {
    "js": "node --harmony"
  },
  "ext": "js json",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "restartable": "rs",
  "verbose": true,
  "watch": [
    "test/fixtures/",
    "test/samples/"
  ]
}
